package com.yash.tdddemo.array;

import java.util.Iterator;

public class Array4 {

	public String reverseString(String string) {
		
		char[] actualString= string.toCharArray();
		int actualStringSize=actualString.length;
		char[] reverseString=new char[actualStringSize];
		
		for(int i=0;i<actualStringSize;i++) {
			reverseString[actualStringSize-1]=actualString[i];
			actualStringSize-=1;
		}
		String rs= reverseString.toString();
		return rs;
	}
	

}

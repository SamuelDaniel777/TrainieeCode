package com.yash.tdddemo.array;

import java.util.Iterator;

public class Array3 {

	public int[] reverseIt(int[] inputArray)
	{
		int[] reverseArray=null;
		for(int i=inputArray.length;i>1;++i) {
			for(int j=0;j<i-1;j++) {
				reverseArray[j-1]=inputArray[i];
			}
		}
		return reverseArray;
	}
	
}

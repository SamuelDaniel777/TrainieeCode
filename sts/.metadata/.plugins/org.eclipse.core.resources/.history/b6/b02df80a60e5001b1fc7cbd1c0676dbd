package com.java.calculator;

public class SumOfn {
	public Integer calculateSum(Integer n) {
		
		// TODO Auto-generated method stub
			int S1, S2, S3, sum;
			S1 = ((n / 3)) * (2 * 3 + (n / 3 - 1) * 3) / 2;
			S2 = ((n / 5)) * (2 * 5 + (n / 5 - 1) * 5) / 2;
			S3 = ((n / (3 * 5))) * (2 * (3 * 5)
                + (n / (3 * 5) - 1) * (3 * 5))/ 2;
			sum=S1+S2-S3;
			return sum;
		}

	public Integer calculateSum(String string) {
		if (string=="") {
			string="no value entered"
			return 0;
		}
		
	}
		
	}



package com.yash.tdddemo.array;

public class Array5 {
	public static void main(String[] args) {
	String[] ar= {"Tea","Coffee","Juice"};	
	String input="Juice";
	
	for(int i=0;i<ar.length;i++) {
		if(ar[i]==input)
		{
			System.out.println("True");
			break;
		}
		else {
			System.out.println("False");
			break;
		}
	}
	
	}
	
}

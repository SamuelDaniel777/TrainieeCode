package com.yash.tdddemo.array;

class CheckString{
	
	boolean check(String st) {
		
		String[] ar= {"Tea","Coffee","Juice"};	
		String input="Juice";
		Boolean b1=true;
		Boolean b2=false;
		for(int i=0;i<ar.length;i++) {
			if(ar[i]==input)
			{
				return b1;
				
			}
			else {
				return false;
				
			}
	}
	
}

